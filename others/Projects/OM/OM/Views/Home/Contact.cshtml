@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>



<div style="background:white;border-radius:10px">
    <div class="row">
        <div class="col-lg-8 mx-auto">
            <div class="form-group row" style="padding-top:20px; width: 100%;margin-left: 7px;">
                @f.FeedbackText
            </div>
        </div>
    </div>

    <div class="row form-inline">
        <div class="col-lg-8 mx-auto">
            <div class="radio-item">
                <input type="radio" id="ritemn" checked="checked" name="ritem" value="ropt2">
                <label for="ritemn">
                    @Html.RadioButton("rdoYesno" + cnt.ToString(), "Yes", true)
                </label>
            </div>
            <div class="num" style="float:left;">Yes </div>

            <div class="radio-item">
                <input type="radio" id="ritemd" name="ritem" value="ropt2">
                <label for="ritemd">
                    @Html.RadioButton("rdoYesno" + cnt.ToString(), "No", true)
                </label>
            </div>

            <div class="num2">No </div>
       
        </div>
    </div>
</div>